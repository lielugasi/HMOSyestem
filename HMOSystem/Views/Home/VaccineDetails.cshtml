@model IEnumerable<HMO.BL.BE.Vaccine>

@{
    ViewBag.Title = "VaccineDetails";
    Layout = "~/Views/Home/LayoutPage.cshtml";
}

<h2>VaccineDetails</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VacDate)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Producer)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VacDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Producer)
            </td>

        </tr>

    }
    <p>

        @Html.ActionLink("Back to List", "Index")
    </p>

</table>
